@model SCIFTAN.BLL.Models.FREELANCER_CAREER_OBJECTIVE_Model

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <hr />
    <div class="form-horizontal">
        <h4>Career Objective</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.Freelancer_Id)

        <div class="form-group">
            <div class="col-md-10">
                @Html.TextAreaFor(model => model.Career_Objective, new { @class = "form-control", @rows = "5" })
                @Html.ValidationMessageFor(model => model.Career_Objective, "", new { @class = "text-danger" })
            </div>
        </div>
        <input type="submit" value="Save" class="btn btn-primary" />
    </div>
}
<br>
<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
