<hr />
<div>
    <h4>Educational Info</h4>
    <table class="table">
        <tr>
            <th>
                Degree
            </th>
            <th>
                Institute
            </th>
            <th>
                Year
            </th>
            <th>
                Result
            </th>
            <th>
                Subject
            </th>
        </tr>

        @foreach (var item in Model.Educations)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Degree)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Institute)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Year)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Result)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Subject)
                </td>
            </tr>
        }

    </table>
</div>


<hr />
<div>
    <h4>Experinece Info</h4>
    <table class="table">
        <tr>

            <th>
                Organization
            </th>
            <th>
                Designation
            </th>
            <th>
                Responsibilities
            </th>
            <th>
                AreaofExperiences
            </th>
            <th>
                Location
            </th>
            <th>
                FromDate
            </th>
            <th>
                ToDate
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model.Experiences)
