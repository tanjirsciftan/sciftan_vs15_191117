                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Location)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FromDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ToDate)
                </td>
            </tr>
        }

    </table>
</div>


<hr />
<div>
    <h4>Skill Info</h4>
    <table class="table">
        <tr>
            <th>
                SkillName
            </th>
            <th>
                Description
            </th>

        </tr>

        @foreach (var item in Model.Skills)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.SkillName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Description)
                </td>

            </tr>
        }

    </table>
</div>


<hr />
<div>
    <h4>Docs</h4>
    <table class="table">
        <tr>
            <th>
                NID
            </th>
            <th>
                PVC
            </th>
        </tr>
        <tr>
            <td>
                <img src="@Url.Content(Model.Docs.NID_Path)" alt="Image" height="400" width="400" />
            </td>
            <td>
                <img src="@Url.Content(Model.Docs.PVC_Path)" alt="Image" height="400" width="400" />
            </td>
        </tr>

    </table>
